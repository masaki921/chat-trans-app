{
  "expo": {
    "name": "ChatTranslate",
    "slug": "chat-translate-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "chat-translate",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAF8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chattranslate.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryAddUsageDescription": "Save images to your photo library",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAFAF8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chattranslate.app",
      "permissions": []
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Access photos to send in chat and set your profile picture",
          "cameraPermission": "Take photos to send in chat and set your profile picture"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4A6CF7"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3b623790-2907-449c-bc32-feb120afa066"
      }
    },
    "owner": "masaki00",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3b623790-2907-449c-bc32-feb120afa066"
    }
  }
}
